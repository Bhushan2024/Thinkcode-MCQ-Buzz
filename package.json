{
  "name": "kavi2-angular-template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "compodoc": "npx compodoc -p tsconfig.doc.json -s",
    "format": "prettier --write \"src/**/*.{ts,js,json,css,scss,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,css,scss,html}\"",
    "build-libs": "cd projects/kavi2-ui-library && ng build && npm run build && cd ../../dist/kavi2-ui-library/ && npm link && cd ../../ && npm link kavi2-ui-library"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/schematics": "^15.2.0",
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@compodoc/compodoc": "^1.1.25",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@ng-select/ng-select": "^10.0.1",
    "bootstrap": "^4.6.2",
    "copyfiles": "^2.4.1",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "ng-packagr": "^15.2.2",
    "ngx-bootstrap": "10.3.0",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.14.5",
    "ts-morph": "^24.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0",
    "ag-grid-angular": "^30.0.0",
    "ag-grid-community": "^30.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "~15.2.10",
    "@angular/compiler-cli": "^15.2.0",
    "@types/jasmine": "~4.3.0",
    "@types/lodash": "^4.14.171",
    "husky": "^9.1.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "typescript": "~4.9.4"
  }
}
